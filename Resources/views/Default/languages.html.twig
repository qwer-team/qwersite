<ul class="languages" >
    <a href="#">
        <li id="" class="flag-{{ locale | upper }} transparent60">
        </li>
    </a>

    {% for key, url in urls %}
        {% if entity is not empty and entity.translate(key).translit != '' %}
        <a href="{{ url }}">
            <li id="" class="flag-{{ key | upper }}">
            </li>
         </a>
        {% endif %}
    {% endfor %}

</ul>
