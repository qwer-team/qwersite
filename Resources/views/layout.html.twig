<!DOCTYPE HTML>
<html lang="en-US">
<head>

    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE8" />
    <meta charset="UTF-8">
    <link rel="shortcut icon" href="/image/favicon.ico" />
    <title>
     {% if entity is not empty %}
       {% if entity.translate(locale).getMetatitle is not empty %}
            {{ entity.translate(locale).getMetatitle }}
        {% else %}
            translate(locale).getTitle
        {% endif %}  
     {% endif %} 
    </title>
    {% block javascript %}

        <script src="{{ asset ( 'bundles/itcadmin/js/plugins/jquery-1.8.2.min.js?ver=1.8.2' ) }}" > </script>
        <script src="{{ asset ( 'bundles/itcadmin/js/plugins/jquery.fancybox-1.4.3/jquery.fancybox-1.3.4.pack.js' ) }}" ></script>
        <script src="{{ asset ( 'bundles/itcadmin/js/plugins/jquery.fancybox.js' ) }}" > </script>
        <script src="{{ asset ( 'bundles/itcadmin/js/plugins/jquery.nivo.slider.js' ) }}" > </script>
        <script src="{{ asset ( 'bundles/itcadmin/js/plugins/jquery.jcarousel.js' ) }}" > </script>

        <script src="{{ asset ( 'bundles/mainsite/js/jq.scrollTo-min.js') }}"></script>
        <script src="{{ asset ( 'bundles/mainsite/js/jquery.aw-showcase.min.js') }}"></script>
        <script src="{{ asset ( 'bundles/mainsite/js/jquery.prettyPhoto.js' ) }}" ></script>
        <script src="{{ asset ( 'bundles/mainsite/js/plugins/superfish.js?ver=1.4.8' ) }}"></script>
        <script src="{{ asset ( 'bundles/mainsite/js/plugins/jquery.loader.js?ver=1.0' ) }}"></script>

        <script src="{{ asset ( 'bundles/mainsite/js/content.js' ) }}" ></script>
    {% endblock %}

    {% block css %}

        <link rel="stylesheet" href="{{ asset ( 'bundles/mainsite/css/style.css' ) }}" />
        <link rel="stylesheet" href="{{ asset ( 'bundles/mainsite/css/prettyPhoto.css' ) }}" />
        <link rel="stylesheet" href="{{ asset ( 'bundles/itcadmin/css/morestyles.css' ) }}" />
        <link rel="stylesheet" href="{{ asset ( 'bundles/mainsite/css/fancy.css' ) }}" />
        <link rel="stylesheet" href="{{ asset ( 'bundles/mainsite/css/plugins/flags/flags.css' ) }}" />
        {# <link rel="stylesheet" href="{{ asset ( 'css/nivo-slider.css' ) }}" /> #}
        <link rel="stylesheet" href="{{ asset ( 'css/plugins/jquery.fancybox-1.3.4/jquery.fancybox-1.3.4.css' ) }}"/>
        {% endblock %}

    {% include 'MainSiteBundle:Site:gag.html.twig'%}

</head>
<body>
    <div id="main">
        {% block menu %}
        <header id="header">
            
            {% set routing = app.request.attributes.get('_route') %}
            {% render(controller("MainSiteBundle:Default:menu", {"routing" : routing, "req" : app.request, "entity": entity, "_locale": app.request.locale}))  %}
        </header>
        {% endblock %}
        {% block body %} 

        {% endblock %}

    </div>
    {% block footer %}
    <footer id="footer">
           {% render (controller("MainSiteBundle:Default:footer",{ "_locale": app.request.locale})) %}
    </footer>
    {% endblock %}
</body>

</html>
